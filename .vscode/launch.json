{
    "configurations": [
 
        {
            "name": "Docker: Python - General",
            "type": "docker",
            "request": "launch",
            "preLaunchTask": "docker-run: debug",
            "python": {
                "pathMappings": [
                    {
                        "localRoot": "${workspaceFolder}",
                        "remoteRoot": "/nbs"
                    }
                ],
                "projectType": "general"
            }
        },
        {
            "name": "Python - Current File",
            "type": "python",
            "request": "launch",
            "program":"${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args" : [ 
                "--domino_location", "./storage/pix2pix/data/maps"
            ]
        }
    ]
}